<Project>
  <PropertyGroup Label="Globals">
    <OrbisSdkVersion>6.0</OrbisSdkVersion>
  </PropertyGroup>
  <ItemDefinitionGroup Condition="*">
    <ClCompile>
      <AdditionalIncludeDirectories>$(SCE_ORBIS_SDK_DIR)\target\include</AdditionalIncludeDirectories>
      <AdditionalIncludeDirectories>$(SCE_ORBIS_SDK_DIR)\target\include_common</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>_CRT_SECURE_NO_DEPRECATE</PreprocessorDefinitions>
      <PreprocessorDefinitions>_ORBIS</PreprocessorDefinitions>
      <PreprocessorDefinitions>_PS4</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(SCE_ORBIS_SDK_DIR)\target\lib</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Debug|ORBIS'">
    <ClCompile>
      <PreprocessorDefinitions>_DEBUG</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='{Dev,Release,Profile,QA,Final}|ORBIS'">
    <ClCompile>
      <PreprocessorDefinitions>NDEBUG;SCE_GNM_RELEASE</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
